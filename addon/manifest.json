{
  "manifest_version": 2,
  "name": "example_name",
  "version": "1.0",
  "description": "foobar",
  "icons": {
    "48": "icons/example_icon.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "example@foo.bar",
      "strict_min_version": "50.0"
    }
  },
  "background": 
    {
      "scripts": [
        "example_script.js"
      ]
    },
    "permissions": ["nativeMessaging"],
    "browser_action": {
      "default_icon": "icons/example_icon.png"
    }
  
}